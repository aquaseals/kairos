{
    "name" : "rabbithole",
    "description" : "Base Level Extension",
    "version" : "1.0",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "./popup/popup.html",
        "default_icon" : "rabbithole_icon.png"
    },
    "background" : {
        "service_worker": "./background/background.js"
    },
    "permissions" : [
        "tabs", 
        "activeTab",
        "alarm"
    ]
}